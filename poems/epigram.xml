  <teiHeader>
    <fileDesc>
      <titleStmt>
        <title>Epigram Engraved on the Collar of a Dog Which I Gave to His Royal Highness</title>
        <author>Alexander Pope</author>
      </titleStmt>
      <publicationStmt>
        <date>1738</date>
      </publicationStmt>
