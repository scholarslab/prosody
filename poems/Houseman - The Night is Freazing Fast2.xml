            <titleStmt>
                <title>The Night is Freezing Fast</title>
                <author>A. E. Housman</author>
            </titleStmt>
            <publicationStmt>
                <p>N/A</p>
            </publicationStmt>
            <sourceDesc><p/></sourceDesc>
        </fileDesc>
